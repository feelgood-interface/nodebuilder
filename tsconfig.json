{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "lib": [
      "es2019"
    ],
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "downlevelIteration": true,
    "plugins": [
			{ "transform": "typescript-is/lib/transform-inline/transformer" }
		]
  },
  "exclude": [
    "src/input",
    "src/output"
  ]
}
